<div class="articles-list-container">
    <h2 class="text-center">{{ 'search.404_title'|trans }}</h2>
    <div class="articles-list">
        <div class="inner">
            <!-- Start: Page widget  -->
            {% for widget in section.pageWidgets %}
                {% set translation = widget.translation(locale) %}
                {% include 'frontend/pages/partials/404/widgets/' ~ widget.template ignore missing %}
            {% endfor %}
            <!-- End: Page widget  -->
        </div>
    </div>
</div>